modules = ["nodejs-20", "web", "postgresql-16"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-24_05"

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 35025
externalPort = 5000

[[ports]]
localPort = 38943
externalPort = 3002

[[ports]]
localPort = 40549
externalPort = 4200

[[ports]]
localPort = 41663
externalPort = 5173

[[ports]]
localPort = 41935
externalPort = 3001

[[ports]]
localPort = 42933
externalPort = 3000

[[ports]]
localPort = 46375
externalPort = 3003

[env]
PORT = "5000"

[agent]
integrations = ["javascript_openai:1.0.0", "javascript_mem_db:1.0.0", "javascript_object_storage:1.0.0", "javascript_stripe:1.0.0", "javascript_log_in_with_replit:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000
